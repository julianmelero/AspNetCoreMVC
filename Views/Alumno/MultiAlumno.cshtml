@model List<Alumno>

@{
    ViewData["Title"] = "Información Alumnos";
    Layout = "Simple";
}
<h1>@ViewData["Title"]</h1>

@* @foreach(var alumno in Model){
    <partial name= "ObjetoEscuelaSimple" model="alumno" />
    
} *@
<table class="table table-dark table-hover">
    <thead>
        <th scope="col">Nombre</th>
        <th scope="col">Id</th>
    </thead>
    <tbody>
@foreach(var alumno in Model){
    <tr>
        <td>@alumno.Nombre</td>
        <td>@alumno.UniqueId</td>
    </tr>
}
</tbody>
</table>

<p>@ViewBag.OtrosDatos</p>
<p>@ViewData["OtrosDatos"]</p>